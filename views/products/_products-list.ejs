<ul>
  <% for (let product of products) { %>
    <li>
      <div><img src="/images/products/<%= product.featured_image %>" width="120" height="80" /></div>
      <div>
        <a href="/products/<%= product.slug %>"><%= product.name %></a><br/>
        <%= product.category.name %><br/>
        <%= product.dimension %><br/>
        <%= product.material %>
      </div>
    </li>
  <% } %>
</ul>